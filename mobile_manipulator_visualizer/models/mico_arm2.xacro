    <property name="joint_finger_tip_1" value="mico_joint_finger_tip_1"/>
    <property name="joint_finger_tip_1_axis_xyz" value="0 0 1"/>
    <property name="joint_finger_tip_1_origin_xyz" value="0.035 -0.02595 -0.0025"/>
