            </collision>
            <inertial>
                <origin xyz="0.0083 0.0000 -0.0378" rpy="0 0 0"/>
                <mass value="0.415"/>
                <inertia ixx="4.87e-3" ixy="5.56e-4" ixz="1.77e-6"
