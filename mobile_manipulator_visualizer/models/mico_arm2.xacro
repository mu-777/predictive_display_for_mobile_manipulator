    <property name="joint_finger_tip_1_mimic_joint" value="${joint_finger_1}"/>
    <property name="joint_finger_tip_1_mimic_multiplier" value="0.0"/>
    <property name="joint_finger_tip_1_mimic_offset" value="0.0"/>

