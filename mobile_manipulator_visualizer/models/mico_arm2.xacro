                </geometry>
            </collision>
            <inertial>
                <origin xyz="0.0728 0.0000 -0.0077" rpy="0 0 0"/>
                <mass value="0.825"/>
                <inertia ixx="2.57e-2" ixy="-1.26e-6" ixz="-9.81e-3"
                         iyy="5.72e-2" iyz="-4.54e-6"
